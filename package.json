    "@types/react-redux": "~7.1.0",
    "@types/react-router-dom": "~5.1.0",
    "html-webpack-plugin": "~3.2.0",
    "husky": "~4.0.0",
    "lint-staged": "~10.0.0",
    "prettier": "~1.19.0",
    "ts-loader": "~6.2.0",
    "typescript": "~3.7.0",
