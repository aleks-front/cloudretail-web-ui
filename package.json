{
  "name": "cloudretail-web-ui",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "start": "react-scripts start",
    "test": "react-scripts test"
  },
  "dependencies": {
    "@types/classnames": "~2.2.0",
    "@types/jest": "~24.0.0",
    "@types/node": "~13.1.0",
    "@types/react": "~16.9.0",
    "@types/react-dom": "~16.9.0",
    "@types/react-redux": "~7.1.0",
    "@types/react-router-dom": "~5.1.0",
    "classnames": "~2.2.0",
    "husky": "~3.1.0",
    "lint-staged": "~9.5.0",
    "prettier": "~1.19.0",
    "react": "~16.12.0",
    "react-dom": "~16.12.0",
    "react-redux": "~7.1.0",
    "react-router-dom": "~5.1.0",
    "react-scripts": "~3.3.0",
    "redux": "~4.0.0",
    "redux-saga": "~1.1.0",
    "reselect": "~4.0.0",
    "typescript": "~3.7.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version"
    ]
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "lint-staged": {
    "src/**/*.{html,css,scss,json}": [
      "prettier --write",
      "git add"
    ],
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc && lint-staged"
    }
  }
}
